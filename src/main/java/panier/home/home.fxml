<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml"
      fx:controller="panier.home.HomeController">
    <GridPane fx:id="productGridPane" >
        <padding>
            <Insets top="25" right="25" bottom="25" left="25"/>
        </padding>
        <!-- Product Views -->
        <VBox fx:id="productView1" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <VBox fx:id="productView2" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <VBox fx:id="productView3" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
        <VBox fx:id="productView4" GridPane.columnIndex="0" GridPane.rowIndex="1"/>

    </GridPane>
</VBox>
